# GitIgnore API Server - Environment Variables
# Copy this file to .env and customize as needed

# Version information
VERSION=1.0.0
COMMIT=dev
BUILD_DATE=

# Admin credentials (first run only)
ADMIN_USER=administrator
ADMIN_PASSWORD=

# Database configuration
DB_TYPE=sqlite
# For PostgreSQL: postgres://gitignore:password@postgres:5432/gitignore
# For MySQL: mysql://gitignore:password@mysql:3306/gitignore
DATABASE_URL=

# PostgreSQL password (if using postgres service)
POSTGRES_PASSWORD=changeme

# MySQL passwords (if using mysql service)
MYSQL_PASSWORD=changeme
MYSQL_ROOT_PASSWORD=rootchangeme
